#include <iostream>
#include <string>
using namespace std;

int main(){
	string sentence[105];
	int row=0, max_len=0;
	while( getline(cin, sentence[row]) ){
		if( max_len<sentence[row].length() )
			max_len = sentence[row].length();
		row++;
	}
	for(int i=0; i<max_len; i++){
		for(int j=row-1; j>=0; j--){
			if( i<sentence[j].length() )
				cout << sentence[j][i];
			else cout << ' ';
		}
		cout << endl;
	}
}
